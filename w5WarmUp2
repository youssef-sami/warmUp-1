 function filter(coll, predicate) {
 	var acc = []; 
 	if (!Array.isArray(coll)) {
 		acc = {};
 	} 
 	each(coll, function(element,key){
 		if (predicate(element,key)) {
 			acc[key] = element;
 		}
 	});
 	return acc;
 }


